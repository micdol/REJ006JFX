<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<VBox spacing="10.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.dolinskm.rej006.views.OfflineSettingsViewController">
    <children>
        <fx:include fx:id="settingsGrid" source="../controls/settings-grid.fxml"/>
        <HBox spacing="10.0">
            <children>
                <Button fx:id="btnSetSettings" mnemonicParsing="false" onAction="#onSetSettingsClicked" text="Wyślij"/>
                <Button fx:id="btnGetSettings" mnemonicParsing="false" onAction="#onGetSettingsClicked" text="Pobierz"/>
                <Button fx:id="btnStartRegistration" mnemonicParsing="false" onAction="#onStartRegistrationClicked"
                        text="Start..."/>
            </children>
        </HBox>
        <HBox alignment="BOTTOM_RIGHT" VBox.vgrow="ALWAYS">
            <children>
                <Button fx:id="btnBack" mnemonicParsing="false" onAction="#onBackClicked" text="Powrót..."/>
            </children>
        </HBox>
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
</VBox>
