<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<VBox xmlns:fx="http://javafx.com/fxml/1" spacing="10.0" xmlns="http://javafx.com/javafx/11.0.1"
      fx:controller="com.dolinskm.rej006.views.AppSettingsViewController">
    <children>
        <HBox spacing="10.0">
            <children>
                <Label maxHeight="1.7976931348623157E308" minWidth="128.0" text="Katalog archiwum"/>
                <TextField fx:id="txtArchiveDirectory" HBox.hgrow="ALWAYS"/>
                <Button fx:id="btnBrowseArchive" mnemonicParsing="false" onAction="#onBrowseArchive" text="..."/>
            </children>
        </HBox>
        <HBox spacing="10.0">
            <children>
                <Label maxHeight="1.7976931348623157E308" minWidth="128.0" text="Katalog ustawień"/>
                <TextField fx:id="txtSettingsDirectory" HBox.hgrow="ALWAYS"/>
                <Button fx:id="btnBrowseSettings" mnemonicParsing="false" onAction="#onBrowseSettings" text="..."/>
            </children>
        </HBox>
        <HBox spacing="10.0">
            <children>
                <Label maxHeight="1.7976931348623157E308" minWidth="128.0" text="Domyślny port"/>
                <TextField fx:id="txtPort" HBox.hgrow="ALWAYS"/>
                <Button mnemonicParsing="false" opacity="0.0" text="..."/>
            </children>
        </HBox>
        <fx:include fx:id="backPane" source="../controls/back-pane.fxml" VBox.vgrow="ALWAYS"/>
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
</VBox>
