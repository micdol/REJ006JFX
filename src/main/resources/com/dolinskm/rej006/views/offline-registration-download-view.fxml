<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.Spinner?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<VBox spacing="10.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.dolinskm.rej006.views.OfflineRegistrationDownloadView">
    <children>
        <HBox spacing="10.0">
            <children>
                <Spinner fx:id="spnRegistrationNumber"/>
                <Button fx:id="btnDownload" mnemonicParsing="false" onAction="#onDownloadClicked" text="Pobierz"/>
            </children>
        </HBox>
        <ProgressBar fx:id="prgDownloadProgress" maxWidth="1.7976931348623157E308" minHeight="25.0" progress="0.0"/>
        <HBox alignment="TOP_RIGHT" spacing="10.0">
            <children>
                <Button fx:id="btnSave" mnemonicParsing="false" onAction="#onSaveClicked" text="Zapisz..."/>
                <Button fx:id="btnPlot" mnemonicParsing="false" onAction="#onPlotClicked" text="Wykres..."/>
            </children>
        </HBox>
        <HBox alignment="BOTTOM_RIGHT" VBox.vgrow="ALWAYS">
            <children>
                <Button fx:id="btnBack" mnemonicParsing="false" onAction="#onBackClicked" text="PowrÃ³t..."/>
            </children>
        </HBox>
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
</VBox>
