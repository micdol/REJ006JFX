<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Spinner?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane hgap="10.0" vgap="10.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
          fx:controller="com.dolinskm.rej006.controls.SettingsGridController">
    <rowConstraints>
        <RowConstraints fx:id="rowDelay" minHeight="-Infinity"/>
        <RowConstraints fx:id="rowLength" minHeight="-Infinity"/>
        <RowConstraints minHeight="-Infinity"/>
        <RowConstraints minHeight="-Infinity"/>
        <RowConstraints minHeight="-Infinity"/>
        <RowConstraints minHeight="25.0"/>
        <RowConstraints maxHeight="1.7976931348623157E308" minHeight="25.0"/>
        <RowConstraints maxHeight="1.7976931348623157E308" minHeight="25.0" prefHeight="30.0"/>
        <RowConstraints maxHeight="1.7976931348623157E308" minHeight="25.0" prefHeight="30.0"/>
    </rowConstraints>
    <columnConstraints>
        <ColumnConstraints minWidth="-Infinity"/>
        <ColumnConstraints minWidth="-Infinity"/>
        <ColumnConstraints minWidth="-Infinity"/>
        <ColumnConstraints minWidth="-Infinity"/>
    </columnConstraints>
    <children>
        <Label fx:id="lblDelay" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Zwłoka"/>
        <Label fx:id="lblLength" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Długość"
               GridPane.rowIndex="1"/>
        <Label maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Częstotliwość"
               GridPane.rowIndex="2"/>
        <Label maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Akcelerometr"
               GridPane.rowIndex="3"/>
        <Label maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Żyroskop"
               GridPane.rowIndex="4"/>
        <Label maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Kanały"
               GridPane.rowIndex="5"/>
        <ComboBox fx:id="cbxFrequency" maxWidth="1.7976931348623157E308" GridPane.columnIndex="1"
                  GridPane.columnSpan="3" GridPane.rowIndex="2"/>
        <ComboBox fx:id="cbxAccelerometer" maxWidth="1.7976931348623157E308" GridPane.columnIndex="1"
                  GridPane.columnSpan="3" GridPane.rowIndex="3"/>
        <ComboBox fx:id="cbxGyroscope" maxWidth="1.7976931348623157E308" GridPane.columnIndex="1"
                  GridPane.columnSpan="3" GridPane.rowIndex="4"/>
        <Spinner fx:id="spnLength" editable="true" maxWidth="1.7976931348623157E308" GridPane.columnIndex="1"
                 GridPane.columnSpan="3" GridPane.rowIndex="1"/>
        <Spinner fx:id="spnDelay" editable="true" maxWidth="1.7976931348623157E308" GridPane.columnIndex="1"
                 GridPane.columnSpan="3"/>
        <CheckBox fx:id="chkX" maxWidth="1.7976931348623157E308" minHeight="25.0" mnemonicParsing="false" text="X"
                  GridPane.columnIndex="1" GridPane.rowIndex="5"/>
        <CheckBox fx:id="chkRoll" maxWidth="1.7976931348623157E308" minHeight="25.0" mnemonicParsing="false"
                  text="Przechył" GridPane.columnIndex="1" GridPane.rowIndex="6"/>
        <CheckBox fx:id="chkY" maxWidth="1.7976931348623157E308" minHeight="25.0" mnemonicParsing="false" text="Y"
                  GridPane.columnIndex="2" GridPane.rowIndex="5"/>
        <CheckBox fx:id="chkZ" maxWidth="1.7976931348623157E308" minHeight="25.0" mnemonicParsing="false" text="Z"
                  GridPane.columnIndex="3" GridPane.rowIndex="5"/>
        <CheckBox fx:id="chkYaw" maxWidth="1.7976931348623157E308" minHeight="25.0" mnemonicParsing="false" text="Obrót"
                  GridPane.columnIndex="3" GridPane.rowIndex="6"/>
        <CheckBox fx:id="chkPitch" maxWidth="1.7976931348623157E308" minHeight="25.0" mnemonicParsing="false"
                  text="Pochył" GridPane.columnIndex="2" GridPane.rowIndex="6"/>
        <Button fx:id="btnSave" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#onSaveClicked"
                text="Zapisz..." GridPane.columnIndex="3" GridPane.rowIndex="7"/>
        <Button fx:id="btnLoad" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#onLoadClicked"
                text="Załaduj" GridPane.columnIndex="3" GridPane.rowIndex="8"/>
        <ComboBox fx:id="cbxSettings" maxWidth="1.7976931348623157E308" GridPane.columnSpan="3" GridPane.rowIndex="8"/>
    </children>
</GridPane>
